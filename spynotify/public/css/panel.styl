.panel
  position relative
  display block
  width 75%
  border-style solid
  border-color #e9e9e9
  padding 14px 14px 0px 14px
  background #fff
  margin 0 20px 0 20px

  border-width 1px 1px 0px 1px
  border-radius 4px 4px 4px 4px

  &.top
    border-radius 4px 4px 0 0
  &.middle
    border-radius 0
  &.bottom
    border-radius 0 0 4px 4px
